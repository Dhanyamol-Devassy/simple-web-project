{
  "name": "my-serverless-app",
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/server.js" },
    { "src": "/alldata", "dest": "/server.js" },
    { "src": "/viewdata", "dest": "/server.js" },
    { "src": "/country/:countryName/holidays", "dest": "/server.js" },
    { "src": "/country/:countryName/population", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}

  
